<div class="wrapper_div">
	<div class="center_div">
		<table>
			<thead>
			<tr class="row_odd">
				<th colspan="2"><h1 style="margin:20px">VIEW This Technology Executive "Enabler"</h1></th>
			</tr>
			</thead>
			<tbody>
			<tr>
				<td>Prefix or Honorific/Rank:
				</td>
				<td><%= @enabler.prefix %>
				</td>
			</tr>
			<tr class="row_odd">
				<td>First Name:</td>
				<td><%= @enabler.firstname %></td>
			</tr>
			<tr>
				<td>Last Name:</td>
				<td><%= @enabler.lastname %></td>
			</tr>
			<tr class="row_odd">
				<td>Job Title:</td>
				<td><%= @enabler.jobtitle %></td>
			</tr>
			<tr>
				<td>Organization:</td>
				<td><%= @enabler.org %></td>
			</tr>
			<tr class="row_odd">
				<td>Email Address, Main:</td>
				<td><%= @enabler.email %>
				</td>
			</tr>
			<tr>
				<td>Alternate Email:</td>
				<td><%= @enabler.altemail %></td>
			</tr>
			<tr class="row_odd">
				<td>Type of Enabler:</td>
				<td><%= @enabler.enabler_type %></td>
			</tr>
			<tr>
				<td>Mobile Tel:</td>
				<td><%= @enabler.mobiletel %></td>
			</tr>
			<tr class="row_odd">
				<td>Office Tel:</td>
				<td><%= @enabler.officetel %></td>
			</tr>
			<tr>
				<td>Address Street:</td>
				<td><%= @enabler.address_street %></td>
			</tr>
			<tr class="row_odd">
				<td>Address City:</td>
				<td><%= @enabler.address_city %></td>
			</tr>
			<tr>
				<td>Address State:</td>
				<td><%= @enabler.address_state %></td>
			</tr>
			<tr class="row_odd">
				<td>Address Zip:</td>
				<td><%= @enabler.address_zip %></td>
			</tr>
			<tr>
				<td>Major Deals:</td>
				<td><%= @enabler.major_deals %></td>
			</tr>
			<tr class="row_odd">
				<td>Notes:</td>
				<td><%= @enabler.notes %></td>
			</tr>
			<tr style="background-color:#055154">
				<td style="font-weight:normal"><br>
					<%= link_to "Return to the list of Enablers!", enablers_path %></td>
				<td><br>
					<%= link_to "Delete this Enabler!", :enabler, method: :delete, data: {confirm: "Are you sure you want to delete this Enabler?"} %></td>
			</tr>
			</tbody>
		</table>
	</div>
</div>


