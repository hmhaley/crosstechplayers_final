<div class="wrapper_div">
	<div class="center_div">
		<table>
			<thead>
			<tr class="row_odd">
				<th colspan="4"><h1 style="margin:20px">VIEW the Technology "Organizations" enabling Innovation</h1></th>
			</tr>
			</thead>
			<tbody>
		<% @organizations.each do |one_org| %>
			<tr>
				<td><strong><%= one_org.name %></strong>
				</td>
				<td><strong><%= one_org.type %></strong>
				</td>
				<td><%= one_org.web_url %></strong>
				</td>
				<td><%= one_org.description %></strong>
				</td>
			</tr>
			<% end %>
			</tbody>
		</table>
	</div>
</div>